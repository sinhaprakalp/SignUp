  </servlet>
   <servlet>
  	<servlet-name>downloadresumeservlet</servlet-name>
  	<display-name>downloadresumeservlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.downloadresumeservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveSignUpServlet</servlet-name>
  	<url-pattern>/SaveSignUpServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginservlet</servlet-name>
  	<url-pattern>/loginservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HrLoginServlet</servlet-name>
  	<url-pattern>/HrLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminLoginServlet</servlet-name>
  	<url-pattern>/AdminLoginServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>downloadresumeservlet</servlet-name>
  	<url-pattern>/downloadresumeservlet</url-pattern>
  </servlet-mapping>
</web-app>
