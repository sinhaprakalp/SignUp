<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveSignUpServlet</servlet-name>
  	<display-name>SaveSignUpServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.SaveSignUpServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginservlet</servlet-name>
  	<display-name>loginservlet</display-name>
  	<description></description>
