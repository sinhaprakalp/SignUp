  </servlet>
  <servlet>
  	<servlet-name>HrLoginServlet</servlet-name>
  	<display-name>HrLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.HrLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminLoginServlet</servlet-name>
  	<display-name>AdminLoginServlet</display-name>
  	<description></description>
